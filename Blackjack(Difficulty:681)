import java.util.*;
import java.lang.*;
import java.io.*;

class Codechef
{
	public static void main (String[] args) throws java.lang.Exception
	{
		// your code goes here
	    Scanner sc=new Scanner(System.in);
	    int T=sc.nextInt();
	    while(T-->0)
	    {
	        int A=sc.nextInt();
	        int B=sc.nextInt();
	        int ans=21-(A+B);
	        if(ans<=10)
	        {
	            System.out.println(ans);
	        }
	        else
	        {
	            System.out.println(-1);
	        }
	    }

	}
}
