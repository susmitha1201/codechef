import java.util.*;
import java.lang.*;
import java.io.*;

class Codechef
{
	public static void main (String[] args) throws java.lang.Exception
	{
		// your code goes here
		Scanner sc=new Scanner(System.in);
		int T=sc.nextInt();
		while(T-->0)
		{
		    int Na=sc.nextInt();
		    int Nb=sc.nextInt();
		    int Nc=sc.nextInt();
		    if(Nc+Nb<Na || Na+Nb<Nc || Na+Nc<Nb)
		    {
		        System.out.println("YES");
		    }
		    else
		    {
		        System.out.println("NO");
		    }
		}

	}
}
