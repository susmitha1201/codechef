import java.util.*;
import java.lang.*;
import java.io.*;

class Codechef
{
	public static void main (String[] args) throws java.lang.Exception
	{
		// your code goes here
		Scanner sc=new Scanner(System.in);
		long T=sc.nextLong();
		while(T-->0)
		{
		    int X=sc.nextInt();
		    int Y=sc.nextInt();
		    double p=(1.07)*X;
		    if(p>=Y)
		    {
		        System.out.println("YES");
		    }
		    else
		    {
		        System.out.println("NO");
		    }
		}

	}
}
