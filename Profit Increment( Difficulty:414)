import java.util.*;
import java.lang.*;
import java.io.*;

class Codechef
{
	public static void main (String[] args) throws java.lang.Exception
	{
		// your code goes here
		Scanner sc=new Scanner(System.in);
		int T=sc.nextInt();
		while(T-->0)
		{
		    int X=sc.nextInt();
		    int Y=sc.nextInt();
		    int p = X * 10 / 100;
            int t = X + p;
            int d = X - Y;
            int k = t - d;
            System.out.println(k);
		}

	}
}
