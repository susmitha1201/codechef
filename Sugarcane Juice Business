import java.util.*;
import java.lang.*;
import java.io.*;

class Codechef
{
	public static void main (String[] args) throws java.lang.Exception
	{
		// your code goes here
		Scanner sc=new Scanner(System.in);
		int T=sc.nextInt();
		while(T-->0)
		{
		    int N=sc.nextInt();
		    int k=50*N;
		    int two=(int)(0.2*k);
		    int three=(int)(0.3*k);
		    int total=(k-(2*two+three));
		    System.out.println(total);
		}

	}
}
