import java.util.*;
import java.lang.*;
import java.io.*;

class Codechef
{
	public static void main (String[] args) throws java.lang.Exception
	{
		// your code goes here
		Scanner sc=new Scanner(System.in);
		int X=sc.nextInt();
		String s1=Integer.toString(X);
		String s2=s1.substring(s1.length()-2);
		System.out.println('k'+s2);
		

	}
}
